
{
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "react:recommended",
      "plugin:react-native/all"
    ],
    "plugins": [
      "babel",
      "react",
      "react-native"
    ],
    "globals": {
      "fetch": true,
      "__DEV__": true
    },
    "rules": {
      "react-native/no-unused-styles": 2,
      "react-native/split-platform-components": 2,
      "react-native/no-inline-styles": 2,
      "react-native/no-color-literals": 2,
      "no-unused-vars":0,
      "no-const-assign":0,
      "no-mixed-spaces-and-tabs":0
    }
}
  